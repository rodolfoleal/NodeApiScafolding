{
  "name": "typescript-course",
  "version": "1.0.0",
  "description": "API em typescript node, es6, sequelize, postgree",
  "main": "server/server.ts",
  "scripts": {
    "start": "SET NODE_ENV=development& ./node_modules/.bin/ts-node ./server/server.ts",
    "builded": "SET NODE_ENV=development& node ./build/server.js",
    "unit-test": "SET NODE_ENV=test& mocha --opts build/tests/unit/config/mocha.opts ./build/tests/unit/unit.test.js",
    "integration-test": "SET NODE_ENV=test& mocha --opts build/tests/integration/config/mocha.opts ./build/tests/integration/Integration.test.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "API",
    "typescript",
    "Node",
    "es6",
    "sequelize"
  ],
  "author": "Rodolfo Leal",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.18.3",
    "express": "^4.16.3",
    "http": "0.0.0",
    "morgan": "^1.9.1",
    "pg": "^7.4.3",
    "sequelize": "^4.38.1",
    "sequelize-cli": "^4.1.1"
  },
  "devDependencies": {
    "@types/body-parser": "^1.17.0",
    "@types/chai": "^4.1.5",
    "@types/express": "^4.16.0",
    "@types/mocha": "^5.2.5",
    "@types/morgan": "^1.7.35",
    "@types/sequelize": "^4.27.25",
    "@types/supertest": "^2.0.6",
    "chai": "^4.1.2",
    "mocha": "^5.2.0",
    "supertest": "^3.3.0",
    "testdouble": "^3.8.1",
    "ts-node": "^7.0.1",
    "typescript": "^3.0.3"
  }
}
